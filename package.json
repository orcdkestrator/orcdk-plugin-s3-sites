  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-cloudfront": "^3.0.0",
    "@aws-sdk/client-s3": "^3.0.0",
    "glob": "^11.0.3",
    "mime-types": "^2.1.35"
  },
  "peerDependencies": {
    "@orcdkestrator/core": "^1.0.2"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
