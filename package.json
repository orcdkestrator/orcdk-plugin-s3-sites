{
  "name": "@orcdkestrator/orcdk-plugin-s3-sites",
  "version": "1.0.0",
  "description": "S3 static site deployment plugin with CloudFront integration for Orcdkestrator",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "npm run clean && tsc",
