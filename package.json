    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint 'src/**/*.ts'",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "orcdkestrator",
    "plugin",
    "s3",
    "cloudfront",
